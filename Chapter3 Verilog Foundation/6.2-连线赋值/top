    %add;
    %store/vec4 v00000257f17e30e0_0, 0, 16;
    %load/vec4 v00000257f17e3ba0_0;
    %pad/u 32;
    %cmpi/e 5, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %deassign v00000257f18fba70_0, 0, 16;
T_1.2 ;
    %delay 10, 0;
