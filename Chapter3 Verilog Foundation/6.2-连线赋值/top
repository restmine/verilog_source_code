    %sub;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v00000257f17e3ba0_0;
    %pushi/vec4 1, 0, 16;
    %add;
    %store/vec4 v00000257f17e3ba0_0, 0, 16;
